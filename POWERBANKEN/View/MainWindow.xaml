        <TextBox x:Name="Tbx_OrderDate" HorizontalAlignment="Left" Height="36" TextWrapping="Wrap" Text="emptyname.." VerticalAlignment="Top" Width="256" Margin="260,457,0,0" TextChanged="TextBox_TextChanged" FontSize="20" FontStyle="Italic"/>
        <TextBox x:Name="Tbx_LastDayOfOrder" HorizontalAlignment="Left" Height="36" TextWrapping="Wrap" Text="emptyname.." VerticalAlignment="Top" Width="256" Margin="260,498,0,0" TextChanged="TextBox_TextChanged" FontSize="20" FontStyle="Italic"/>
