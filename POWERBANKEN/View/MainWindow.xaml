        <TextBox x:Name="Tbx_ProductName" HorizontalAlignment="Left" Height="36" TextWrapping="Wrap" Text="emptyname.." VerticalAlignment="Top" Width="256" Margin="260,272,0,0" TextChanged="TextBox_TextChanged" FontSize="20" FontStyle="Italic"/>
        <TextBox x:Name="Tbx_ProductID" HorizontalAlignment="Left" Height="36" TextWrapping="Wrap" Text="emptyname.." VerticalAlignment="Top" Width="256" Margin="260,319,0,0" TextChanged="TextBox_TextChanged" FontSize="20" FontStyle="Italic"/>
