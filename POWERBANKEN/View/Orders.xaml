<Window x:Class="View.Orders"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="Orders" Height="600" Width="900">
    <Grid>
        <Label x:Name="Lbl_ProductHeadline" Content="Varebeholdning: " HorizontalAlignment="Left" VerticalAlignment="Top" Height="48" Width="224" Margin="34,49,0,0" FontSize="20" FontWeight="Bold"/>
